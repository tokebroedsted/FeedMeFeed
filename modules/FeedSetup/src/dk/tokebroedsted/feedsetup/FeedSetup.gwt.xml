<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="feedsetup">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="dk.tokebroedsted.commons.Commons"/>


    <!-- Specify the app entry point class.                   -->
    <entry-point class='dk.tokebroedsted.feedsetup.client.FeedSetupEntryPoint'/>


    <!-- Specify the app servlets.                   -->
    <servlet path='/FeedSetupService' class='dk.tokebroedsted.feedsetup.server.FeedSetupServiceImpl'/>

    <stylesheet src="FeedSetup.less"/>
    <set-property name="user.agent" value="safari"/>
</module>